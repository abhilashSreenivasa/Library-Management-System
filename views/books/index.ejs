<h2>Search Book</h2>
<form class="form" action="/books" method="GET">
  <div class="form-field">
    <label>Title</label>
    <input class="form-field-box" type='text' name='title' value ='<%=searchOptions.title %>'>
  </div>
  <div class="form-field">
    <label>Published After</label>
    <input class="form-field-box" type='date' name='publishedAfter' value ='<%=searchOptions.publishedAfter %>'>
  </div>
  <div class="form-field">
    <label>Published before</label>
    <input class="form-field-box" type='date' name='publishedBefore' value ='<%=searchOptions.publishedBefore %>'>
  </div>
    <button class="submit-btn" type='submit'>Search</button>
</form>
<br>
<div>
<% books.forEach( book=> { %>
  <a href="/books/<%=book.id%>">
    <img class="book-image" height='200' width="150" src="<%= book.coverImagePath %>" />
  </a>
    <% }) %>
</div>